{
  "name": "sdb-x-sdk",
  "version": "1.0.0",
  "description": "Powerful TypeScript SDK for managing isolated code execution environments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run build:esm",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --out docs src",
    "validate": "npm run type-check && npm run lint && npm run test"
  },
  "keywords": [
    "sdk",
    "sandbox",
    "isolated",
    "environment",
    "code-execution",
    "typescript",
    "socket.io",
    "container",
    "docker",
    "cloud-ide",
    "e2b",
    "daytona"
  ],
  "author": "utkarsh yadav",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/utkarshya24/sdb-x-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/utkarshya24/sdb-x-sdk/issues"
  },
  "homepage": "https://github.com/utkarshya24/sdb-x-sdk#readme",
  "dependencies": {
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.25",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
